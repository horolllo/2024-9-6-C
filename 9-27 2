#include <stdio.h>
int main(){
	int num1, num2; 
	
	
	printf("첫번쨰 수를 입력하세요 : ");
	scanf("%d", &num1);
	
	printf("두번쨰 수를 입력하세요 : ");
	scanf("%d", &num2);
	
	printf("%d + %d = %d\n", num1, num2, num1 + num2);
	printf("%d - %d = %d\n", num1, num2, num1 - num2);
	printf("%d * %d = %d\n", num1, num2, num1 * num2);
	printf("%d / %d = %d\n", num1, num2, num1 / num2);
	printf("%d % %d = %d\n", num1, num2, num1 % num2);
	
	return 0;
}
