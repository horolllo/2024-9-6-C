#include <stdio.h>

int main() {
//1
  
  printf("%d\n", 80 - 100 / 5 * 3 >= 15);//(1)
  
  printf("%d\n", 1872 - 888 * 2 < 972 - 888);//(2)

  printf("%d\n", 250 == 200 + 100 + 60);//(3)

  printf("%d\n", 170 <= 176 && 95 >= 67);//(4)
  
  printf("%d\n", 8 == 10 && 8 <= 10);//(5)

  printf("%d\n", (85.5 + 87.5 + 90.5 + 91) / 4 > 88 && 90.5 > (85.5 + 87.5 + 90.5 + 91) / 4);//(6)
  
  
  
  
  return 0;
}
