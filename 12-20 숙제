#include <stdio.h>

void InputInt(int pn){
	
}

void InputDouble(double pd){
	
}

void PrintInt(int pn){
	printf("%d", pn);
}

void PrintDouble(double pd){
	printf("%lf", pd);
}

void ChangeInt(int pn){
	printf("%d", pn * -1);
}

void ChangeDouble(double pd){
	printf("%lf", pd * -1.0);
}


int main(){
	
	int n;
	double d;
	
	int pn = &n;
	double pd = &d;
	
	InputInt(pn);
	
	InputDouble(pd);
	
	PrintInt(pn);
	
	PrintDouble(pd);
	
	ChangeInt(pn);
	
	ChangeDouble(pd);
	
	return 0;
}
