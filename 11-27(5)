#include <stdio.h>
int main(){
	
	int i, j, ban;
	int a = 0;
	int ar[6][9] = {{23, 22, 27, 21, 24},
					{21, 22, 25, 24, 23},
					{20, 23, 22, 21, 24},
					{25, 26, 24, 27, 28},
					{26, 24, 28, 29, 26},
					{27, 28, 29, 26, 27}};
	
	scanf("%d", &ban);
	
	for(i = 0; i <6; i++){
			printf("%d학년 %d반 : %d명\n", i+1, ban, ar[i][ban-1]);
		}
		

	return 0;
} 
