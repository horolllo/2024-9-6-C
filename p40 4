#include <stdio.h>

int main(){
	
	int a;
	int b;
	
	scanf("%d %d", &a, &b);
	if(a>b){
		if(a%b==0){
			printf("%d는 %d의 약수\n", b, a);
		}
		else{
			printf("%d는 %d의 약수가 아님\n", b, a);
		}
	}
	else{
		if(b%a==0){
			printf("%d는 %d의 약수\n", a, b);
		}
		else{
			printf("%d는 %d의 약수가 아님\n", a, b);
		}
		
	} 
	
	
	
	
	
	
	
	return 0; 
}
